{
  "name": "validity_checks",
  "version": "1.0.0",
  "description": "Tests the input for correct and reasonable conditions, such as isObject, isFunction etc...",
  "main": "./index.js",
  "scripts": {
    "buildClient": "browserify index.js -t babelify --outfile ./dist/validityChecks.js -s __validityChecks",
    "preversion": "npm run lint && npm run test && npm run buildClient",
    "lint": "./node_modules/jscs/bin/jscs src -x",
    "test": "./node_modules/mocha/bin/_mocha --full-trace --recursive ./src/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://MikeFielden@github.com/mridevteam/validity_checks.git"
  },
  "keywords": [
    "validity",
    "checks"
  ],
  "author": "MriDevTeam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mridevteam/validity_checks/issues"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.6.0",
    "jscs": "^2.11.0",
    "mocha": "^2.5.3"
  },
  "homepage": "https://github.com/mridevteam/validity_checks#readme"
}
